{
  "expo": {
    "name": "Healthy Lifestyle",
    "slug": "healthy-lifestyle",
    "version": "1.0.1",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icons/logo.jpg",
    "splash": {
      "image": "./assets/images/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.qqlmklz.healthy-lifestyle",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app requires access to the camera to take photos.",
        "NSPhotoLibraryUsageDescription": "This app requires access to your photo library to select pictures."
      }
    },
    "android": {
      "package": "com.qqlmklz.healthy-lifestyle",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/logo.jpg",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "custom": {
            "Roboto": "./assets/fonts/Roboto-Regular.ttf"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d9ee2e61-321b-42b8-9ef2-c65d0357c76a"
      }
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "newArchEnabled": true,
    "owner": "qqlmklz"
  }
}
